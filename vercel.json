{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*\\.(css|js|html|png|jpg|jpeg|gif|svg|ico))",
            "dest": "/static/$1"
        },
        {
            "src": "/",
            "dest": "/static/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/static/$1"
        }
    ]
}